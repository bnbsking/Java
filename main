import java.util.Scanner;

public class Scanner_test {
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        int num1, num2;
        
        System.out.println("Enter a number:");
        num1 = sc.nextInt();
        num2 = sc.nextInt();
        System.out.println(num1);
        System.out.println(num2);
        
    }
}
